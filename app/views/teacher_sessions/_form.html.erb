<%= form_for(@teacher_session) do |f| %>
  <%= render :partial => 'layouts/error_block', :locals => {:obj => @teacher_session} %>
  <div class="field">
    <%= f.label :email %><br/>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br/>
    <%= f.password_field :password %>
  </div>
  <div class="field">
    <%= f.submit "Login", :class => "button blue_button" %>
  </div>
<% end %>