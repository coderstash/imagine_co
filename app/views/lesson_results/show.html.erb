<div class="return_panel round">
  <div class="close"></div>
  <h3><%= @lesson.name %></h3>
  <ul>
    <% @lesson.questions.each do |q| %>
      <li class="listed_result">
        <h4><%= q.question %> - <%= q.question_type %></h4>
        <p><%= q.student_result(@lesson.id) %></p>
      </li>
    <% end %>
  </ul>
</div>