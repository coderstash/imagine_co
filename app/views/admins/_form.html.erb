<%= form_for(@admin) do |f| %>
  <%= render :partial => 'layouts/form_errors', :locals => {:obj => @admin} %>

  <div class="field">
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.text_field :password %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
