<li class="listed_student round">
   <div class="student_info student_panel">
     <div class="students_name">
      <%= link_to teacher.full_name, teacher_path(teacher), :class => "view_teacher" %> 
      <%= link_to "Delete Teacher", teacher, :method => 'delete', :confirm => 'Are you sure you want to delete this student?', :title => "Delete Student", :class => "remove_button red" if current_teacher.admin %></div>
     <ul class="student_creds">
       <li>Login: <%= teacher.username %></li>
       <li>Last Login: <%= (teacher.last_login_at).to_s(:pretty) unless teacher.last_login_at.blank?  %></li>
     </ul>
   </div>
   <div class="clear"></div>
 </li>
